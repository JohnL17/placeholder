{{log model}}
<form>
  <fieldset class="uk-fieldset">
    <legend class="uk-legend">My profile</legend>
    <dl class="uk-description-list uk-description-list-divider">
      <dt>First name</dt>
      <dd class="uk-margin-top">{{model.first_name}} </dd>
      <dt>Last name</dt>
      <dd class="uk-margin-top">{{model.last_name}}</dd>
      <dt>Username</dt>
      <dd class="uk-margin-top">{{model.username}}</dd>
      <dt>Email</dt>
      <dd class="uk-margin-top">{{model.email}}</dd>
    </dl>
  </fieldset>
</form>